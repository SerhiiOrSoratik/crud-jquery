{
  "compilerOptions": {
    "outDir": "./lib",
    "allowJs": true,
    "target": "esnext",
    "pretty": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
}
